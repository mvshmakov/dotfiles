# Using TouchId for SSH
Host *
   AddKeysToAgent yes
   UseKeychain yes
   Compression yes
   IdentityFile ~/.ssh/id_rsa
   ForwardAgent yes

Host keenetic
   HostName mvshmakov.keenetic.pro
   User admin
   Port 2022

Host localhost
   HostName localhost
   User mvshmakov
   Port 2222

# Setting up the test accounts on each git provider respectively
Host bitbucket-test.org
  HostName bitbucket.org
  User git
  IdentityFile ~/.ssh/id_rsa-test
  IdentitiesOnly yes

Host github-test.com
  HostName github.com
  User git
  IdentityFile ~/.ssh/id_rsa-test
  IdentitiesOnly yes

Host gitlab-test.com
  HostName gitlab.com
  User git
  IdentityFile ~/.ssh/id_rsa-test
  IdentitiesOnly yes
